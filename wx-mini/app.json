{
  "pages": [
    "page/basic/index",
    "page/component/index",
    "page/API/index",
    "page/cloud/index",
    "page/empty/index"
  ],
  "subpackages": [
    {
      "root": "packageBasic",
      "pages": [
        "page/lifestyle/index",
        "page/wxml/index",
        "page/wxs/index",
        "page/event/index",
        "page/auth/index"
      ]
    }
  ],
  "preloadRule": {
    "page/component/index": {
      "network": "all",
      "packages": [
        "packageBasic"
      ]
    }
